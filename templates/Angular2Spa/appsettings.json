{
  "Authentication": {
    "AzureAd": {
      "AADInstance": "https://login.microsoftonline.com/",
      "CallbackPath": "/callback",
      "ClientId": "<client-id>",
      "ClientSecret": "<client-secret>",
      "Domain": "<tenant.onmicrosoft.com>",
      "TenantId": "<tenant-id>"
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}
